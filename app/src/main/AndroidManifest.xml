<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.CollisionCatcher"
        tools:targetApi="31">
        <activity
            android:name=".Acitivities.EditProfileActivity"
            android:exported="false"
            android:label="@string/title_activity_edit_profile"
            android:theme="@style/Theme.CollisionCatcher" />
        <activity
            android:name=".Acitivities.AddMemberActivity"
            android:exported="false"
            android:label="@string/title_activity_add_memeber"
            android:theme="@style/Theme.CollisionCatcher" />
        <activity
            android:name=".Acitivities.ProfileActivity"
            android:exported="false"
            android:label="@string/title_activity_profile"
            android:theme="@style/Theme.CollisionCatcher" />
        <activity
            android:name=".Acitivities.SettingsActivity"
            android:exported="false"
            android:label="@string/title_activity_settings"
            android:theme="@style/Theme.CollisionCatcher" />
        <activity
            android:name=".Acitivities.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.CollisionCatcher">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Acitivities.TheftProtectionActivity"
            android:exported="false"
            android:label="@string/title_activity_theft_protection"
            android:theme="@style/Theme.CollisionCatcher" />
        <activity
            android:name=".Acitivities.SpeedTrackingActivity"
            android:exported="false"
            android:label="@string/title_activity_speed_tracking"
            android:theme="@style/Theme.CollisionCatcher" />
        <activity
            android:name=".Acitivities.HomeActivity"
            android:exported="false"
            android:label="@string/title_activity_home"
            android:theme="@style/Theme.CollisionCatcher" />
        <activity
            android:name=".Acitivities.ForgotPasswordActivity"
            android:exported="false"
            android:label="@string/title_activity_forgot_password"
            android:theme="@style/Theme.CollisionCatcher" />
        <activity
            android:name=".Acitivities.SignupActivity"
            android:exported="false"
            android:label="@string/title_activity_signup"
            android:theme="@style/Theme.CollisionCatcher" />
        <activity
            android:name=".Acitivities.LoginActivity"
            android:exported="true"
            android:label="@string/title_activity_login"
            android:theme="@style/Theme.CollisionCatcher"></activity>
    </application>

</manifest>